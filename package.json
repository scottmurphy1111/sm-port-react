{
  "homepage": "https://scottmurphy1111.github.io/sm-port-react/",
  "name": "sm-port-react",
  "version": "1.0.0",
  "description": "Scott Murphy Front End and UI Development",
  "author": "Scott Murphy",
  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "jest": {
    "verbose": true,
    "testUrl": "http://localhost",
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "react-scripts build",
    "testOLD": "react-scripts test --env=jsdom",
    "test": "jest --watch",
    "verify-tests": "jest --coverage",
    "eject": "react-scripts eject",
    "sass": "node-sass -w src/styles/scss -o src/styles/css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.13.1",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
